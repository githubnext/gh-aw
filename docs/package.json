    "mermaid": "^11.12.1",
    "sharp": "^0.34.2",
    "starlight-blog": "^0.25.2",
    "starlight-changelogs": "^0.2.1",
    "starlight-links-validator": "^0.17.2",
    "starlight-llms-txt": "^0.6.0",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@marp-team/marp-cli": "^4.2.3",
    "@playwright/test": "^1.56.1",
    "@types/primer__octicons": "^19.11.1",
    "http-server": "^14.1.1",
    "starlight-github-alerts": "^0.1.0"
  }

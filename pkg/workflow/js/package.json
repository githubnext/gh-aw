{
  "devDependencies": {
    "@actions/core": "^1.11.1",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^6.0.1",
    "@actions/glob": "^0.5.0",
    "@actions/io": "^1.1.3",
    "@modelcontextprotocol/sdk": "^1.18.1",
    "@types/node": "^24.5.2",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "vite": "^7.1.6",
    "vitest": "^3.2.4"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "compile": "tsc --project tsconfig.build.json && sed -i 's/^export {};$//' *.js && prettier --write '*.js'",
    "test": "npm run typecheck && vitest run",
    "test:js": "vitest run",
    "test:js-watch": "vitest",
    "test:js-coverage": "vitest run --coverage",
    "format:cjs": "prettier --write '**/*.cjs' '**/*.js' '**/*.ts'",
    "lint:cjs": "prettier --check '**/*.cjs' '**/*.ts'"
  }
}

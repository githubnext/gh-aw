{
  "mcpServers": {
    "gh-aw": {
      "command": "gh",
      "args": ["aw", "mcp-server"],
      "env": {
        "DEBUG": "cli:*"
      }
    },
    "remote-server": {
      "url": "http://localhost:3000"
    },
    "docker-server": {
      "container": "mcp-server:latest",
      "args": ["--verbose"],
      "env": {
        "LOG_LEVEL": "debug"
      }
    }
  },
  "port": 8088
}
